{
  "name": "backendrestaurante",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./src/index",
    "dev": "nodemon --env-file .env ./src/index.js",
    "build": "npx prisma generate && npx prisma db push",
    "build-prod": "npx prisma migrate deploy",
    "init-roles": "node scripts/init-roles.js",
    "test": "node scripts/test-apis.js",
    "test:roles": "node scripts/test-roles.js",
    "test:users": "node scripts/test-users.js",
    "test:auth": "node scripts/test-auth.js",
    "test:aigame": "node scripts/test-aigame.js",
    "test:swagger": "node scripts/test-swagger.js",
    "test:aigame-endpoints": "node scripts/test-aigame-endpoints.js",
    "test:game-generation": "node scripts/test-game-generation.js",
    "populate:production": "node scripts/populate-production-games.js",
    "verify:production": "node scripts/verify-production-data.js",
    "reset:production": "node scripts/reset-production-data.js",
    "setup:production": "npm run populate:production && npm run verify:production",
    "fresh:production": "node scripts/reset-production-data.js --force && npm run setup:production",
    "demo:production": "node scripts/demo-production-setup.js",
    "deploy:validate": "node scripts/deploy-render.js",
    "deploy:optimize": "node scripts/deploy-render.js --create-optimized"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fal-ai/client": "^1.6.1",
    "@google/generative-ai": "^0.24.1",
    "@openzeppelin/contracts": "^5.4.0",
    "@prisma/client": "^6.6.0",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "prisma": "^6.6.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
